+static int lcddrv_write(cfe_devctx_t *ctx,iocb_buffer_t *buffer)
+{
+    lcddev_t *lcd = ctx->dev_softc;
+    fbcon_t *con = &lcd->con;
+
+    unsigned char *bptr;
+    int blen;
+
+    bptr = buffer->buf_ptr;
+    blen = buffer->buf_length;
+
+    while ( blen > 0 )
+    {
+        fbcon_putc(con, *bptr);
+
+        bptr++;
+        blen--;
+    }
+
+    buffer->buf_retlen = buffer->buf_length - blen;
+
+    lcd_sync(lcd);
+
+    return 0;
+}
+static int lcddrv_ioctl(cfe_devctx_t *ctx,iocb_buffer_t *buffer)
+{
+    int ret = 0;
+
+    switch ((int)buffer->buf_ioctlcmd)
+    {
+    default:
+        ret = 0;
+        break;
+    }
+
+    return ret;
+}
+/*
+ * close console
+ */
+static int lcddrv_close(cfe_devctx_t *ctx)
+{
+    return 0;
+}
