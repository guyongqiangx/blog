diff -uNr bcm97584_cfe_v3.7/CFE/cfe/include/dev_lcd.h bcm97584_cfe_v3.7-simplefb/CFE/cfe/include/dev_lcd.h
--- bcm97584_cfe_v3.7/CFE/cfe/include/dev_lcd.h	1970-01-01 08:00:00.000000000 +0800
+++ bcm97584_cfe_v3.7-simplefb/CFE/cfe/include/dev_lcd.h	2016-11-11 11:26:47.000000000 +0800
@@ -0,0 +1,119 @@
+#ifndef __DEV_LCD_H__
+#define __DEV_LCD_H__
+
+#define LCD_MONOCHROME  0
+#define LCD_COLOR2      1
+#define LCD_COLOR4      2
+#define LCD_COLOR8      3
+#define LCD_COLOR16     4
+#define LCD_COLOR32     5
+
+/* 16 bits one pixel */
+#define LCD_BPP         LCD_COLOR16
+
+/* Calculate nr. of bits per pixel and nr. of colors */
+#define NBITS(bit_code)		(1 << (bit_code))
+#define NCOLORS(bit_code)	(1 << NBITS(bit_code))
+
+/* Calculate nr. of bytes per pixel */
+#define NBYTES(bit_code)		(NBITS(bit_code) >> 3)
+
+#if LCD_BPP == LCD_COLOR8
+# define CONSOLE_COLOR_BLACK	0
+# define CONSOLE_COLOR_RED	1
+# define CONSOLE_COLOR_GREEN	2
+# define CONSOLE_COLOR_YELLOW	3
+# define CONSOLE_COLOR_BLUE	4
+# define CONSOLE_COLOR_MAGENTA	5
+# define CONSOLE_COLOR_CYAN	6
+# define CONSOLE_COLOR_GREY	14
+# define CONSOLE_COLOR_WHITE	15		/* Must remain last / highest */
+#elif LCD_BPP == LCD_COLOR32
+#define CONSOLE_COLOR_RED	0x00ff0000
+#define CONSOLE_COLOR_GREEN	0x0000ff00
+#define CONSOLE_COLOR_YELLOW	0x00ffff00
+#define CONSOLE_COLOR_BLUE	0x000000ff
+#define CONSOLE_COLOR_MAGENTA	0x00ff00ff
+#define CONSOLE_COLOR_CYAN	0x0000ffff
+#define CONSOLE_COLOR_GREY	0x00aaaaaa
+#define CONSOLE_COLOR_BLACK	0x00000000
+#define CONSOLE_COLOR_WHITE	0x00ffffff	/* Must remain last / highest */
+#else /* 16bpp color definitions */
+# define CONSOLE_COLOR_BLACK	0x0000
+# define CONSOLE_COLOR_RED	0xF800
+# define CONSOLE_COLOR_GREEN	0x07E0
+# define CONSOLE_COLOR_YELLOW	0xFFE0
+# define CONSOLE_COLOR_BLUE	0x001F
+# define CONSOLE_COLOR_MAGENTA	0xF81F
+# define CONSOLE_COLOR_CYAN	0x07FF
+# define CONSOLE_COLOR_GREY	0xC618
+# define CONSOLE_COLOR_WHITE	0xffff		/* Must remain last / highest */
+#endif /* color definitions */
+
+#if LCD_BPP == LCD_COLOR16
+#define fbptr_t uint16_t
+#elif LCD_BPP == LCD_COLOR32
+#define fbptr_t uint32_t
+#else
+#define fbptr_t uint8_t
+#endif
+
+#define CONSOLE_SCROLL_LINES    1
+
+typedef struct win_s {
+    char *name;
+
+    uint32_t x, y;
+    uint32_t w, h;
+
+    /* by default, use the same bg/fg color as lcd device */
+    /* uint32_t bg_color, fg_color; */
+}win_t;
+
+typedef struct lcd_probe_s {
+    uint8_t rotation;   /* rotation of display (0, 1, 2, 3) */
+    uint8_t bpp;        /* bits per pixel */
+
+    uint32_t width, height;
+
+    uint32_t bg_color, fg_color; /* background/foreground color */
+
+    win_t win;
+
+    void *base;         /* buffer */
+}lcd_probe_t;
+
+
+typedef struct lcddev_s lcddev_t;
+typedef struct fbcon_s fbcon_t;
+
+struct fbcon_s {
+    uint32_t x, y;
+    uint32_t w, h;
+
+    int32_t cols, rows;
+
+    int32_t cur_col, cur_row;
+
+    void (*putc_xy)(fbcon_t *con, uint32_t x, uint32_t y, char c);
+    void (*moverow)(fbcon_t *con, uint32_t rowdst, uint32_t rowsrc);
+    void (*setrow)(fbcon_t *con, uint32_t row, uint32_t color);
+
+    void *start;    /* framebuffer start addr */
+    void *cmap;     /* color map */
+    lcddev_t *lcd;  /* lcd device */
+};
+
+struct lcddev_s {
+    uint8_t rotation;               /* rotation of displays */
+    uint8_t bpp;                    /* bits per pixel */
+    uint32_t width, height;
+    uint32_t pitch;                 /* line pitch in bytes */
+    uint32_t bg_color, fg_color;
+
+    fbcon_t con;                  /* consoles */
+
+    void *base;                     /* buffer */
+};
+
+#endif
