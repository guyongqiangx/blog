+#include <stdarg.h>
+
+#include "lib_types.h"
+#include "lib_printf.h"
+#include "debug.h"
+
+extern void _writeasm(char c);
+static void __puts(char *s)
+{
+    while (*s)
+    {
+        if (*s == '\n')
+            _writeasm('\r');
+        _writeasm(*s++);
+    }
+
+    return;
+}
+
+void serial_printf(const char *fmt, ...)
+{
+    va_list args;
+    char buf[1024];
+
+    va_start(args, fmt);
+    vsprintf(buf, fmt, args);
+    va_end(args);
+
+    __puts(buf);
+
+    return;
+}
+
