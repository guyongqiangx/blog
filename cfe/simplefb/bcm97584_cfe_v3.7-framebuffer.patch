+static void lcd_clear(lcddev_t *lcd)
+{
+#if (LCD_BPP == LCD_COLOR8)
+    memset((char *)lcd->base, lcd->bg_color, lcd->pitch * lcd->height);
+#elif (LCD_BPP == LCD_COLOR16)
+    uint16_t *ppix = lcd->base;
+    uint32_t i;
+    for (i=0; i<(lcd->pitch * lcd->height)/NBYTES(lcd->bpp); i++)
+    {
+        *ppix++ = lcd->bg_color;
+    }
+#else
+    uint32_t *ppix = lcd->base;
+    uint32_t i;
+    for (i=0; i<(lcd->pitch * lcd->height)/NBYTES(lcd->bpp); i++)
+    {
+        *ppix++ = lcd->bg_color;
+    }
+#endif
+    lcd_sync(lcd);
+    return;
+}
+static void lcd_sync(lcddev_t *lcd)
+{
+    CACHE_FLUSH_ALL();
+
+    return;
+}
+
+typedef void bvn_init(BREG_Handle hRegister, BMEM_Handle hHeap, void* ulNewAddress);
+
+static void lcddrv_probe(cfe_driver_t *drv,
+               unsigned long probe_a, unsigned long probe_b,
+               void *probe_ptr)
+{
+    lcddev_t *softc;
+    lcd_probe_t *probe;
+    win_t win;
+
+    bvn_init *init = NULL;
+    char descr[80];
+
+    char buffer[40];
+    BPXL_Format format;
+
+    /*
+     * Now, on with the probing:
+     *  probe_a is the bvn_init function address
+     *  probe_b is unused
+     *  probe_ptr is probe structure pointer
+     */
+
+    probe = (lcd_probe_t *)probe_ptr;
+
+    softc = (lcddev_t *)KMALLOC(sizeof(lcddev_t), 0);
+    if (softc)
+    {
+        memset(softc, 0, sizeof(lcddev_t));
+
+        if (probe)
+        {
+            softc->rotation = probe->rotation;
+            softc->bpp = probe->bpp;
+            softc->width = probe->width;
+            softc->height = probe->height;
+            softc->pitch = probe->width * NBYTES(probe->bpp);
+            softc->bg_color = probe->bg_color;
+            softc->fg_color = probe->fg_color;
+
+            softc->base = probe->base;
+
+            debug("fb range: [0x%08x - 0x%08x]\n", softc->base, (char *)softc->base + softc->pitch * softc->height);
+
+            if (probe->win.w == 0 || probe->win.h == 0)
+            {
+                /* construt a default window */
+                memset(&win, 0, sizeof(win));
+                win.w = probe->width;
+                win.h = probe->height;
+            }
+            else
+            {
+                /* preset window */
+                memcpy(&win, &probe->win, sizeof(win));
+            }
+
+            fbcon_init(&softc->con, &win, softc);
+
+            /* setup simple-framebuffer envs */
+            xsprintf(buffer,"%x", K0_TO_PHYS((unsigned long)softc->base));
+            env_setenv("LCD_SURFACE_ADDR", buffer, ENV_FLG_BUILTIN | ENV_FLG_READONLY);
+            xsprintf(buffer,"%x", softc->pitch * softc->height);
+            env_setenv("LCD_SURFACE_SIZE", buffer, ENV_FLG_BUILTIN | ENV_FLG_READONLY);
+
+            xsprintf(buffer,"%d", softc->width);
+            env_setenv("LCD_DISP_WIDTH",buffer, ENV_FLG_BUILTIN | ENV_FLG_READONLY);
+            xsprintf(buffer,"%d", softc->height);
+            env_setenv("LCD_DISP_HEIGHT", buffer, ENV_FLG_BUILTIN | ENV_FLG_READONLY);
+            xsprintf(buffer,"%d", softc->pitch);
+            env_setenv("LCD_DISP_STRIDE", buffer, ENV_FLG_BUILTIN | ENV_FLG_READONLY);
+
+            splash_get_surf_format(&format);
+            if (format == BPXL_eA8_R8_G8_B8)
+            {
+                xsprintf(buffer, "%s", "a8r8g8b8");
+            }
+            else /* (format == BPXL_eR5_G6_B5 ) */
+            {
+                xsprintf(buffer,"%s", "r5g6b5");
+            }
+            env_setenv("LCD_DISP_FORMAT", buffer, ENV_FLG_BUILTIN | ENV_FLG_READONLY);
+
+            /* description */
+            xsprintf(descr, "%s at 0x%08x, size [%d,%d,%d,%d]",
+                drv->drv_description, softc->base,
+                win.x, win.y, win.x + win.w, win.y + win.h);
+
+            cfe_attach(drv, softc, NULL, descr);
+
+            /* call init */
+            init = (bvn_init *)probe_a;
+            if (init != NULL)
+            {
+                /* reg, heap, surface */
+                init(NULL, NULL, softc->base);
+            }
+
+            /* clear whole screen with background color */
+            lcd_clear(softc);
+        }
+    }
+
+    return;
+}
+
+/*
+ * open console
+ */
+static int lcddrv_open(cfe_devctx_t *ctx)
+{
+    lcddev_t *lcd = ctx->dev_softc;
+
+    fbcon_set_cursor(&lcd->con, 0, 0);
